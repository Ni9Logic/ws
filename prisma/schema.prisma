generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

enum SENSOR_TYPE {
  NODE1
  NODE2
  NODE3
}

model Sensor {
  id Int @id @default(autoincrement())
  type SENSOR_TYPE
  mq135 Int
  mq2 Int
  r1 Int @default(0)
  r2 Int @default(0)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}